{
  "name": "eslint-config-airbnb-extended",
  "version": "3.0.1",
  "description": "Eslint Airbnb Config Extended",
  "keywords": [
    "eslint",
    "airbnb",
    "airbnb config",
    "eslint config",
    "eslint config airbnb",
    "eslint config airbnb base",
    "eslint config airbnb react",
    "eslint config airbnb typescript",
    "eslint config airbnb extended",
    "eslint airbnb",
    "eslint airbnb base",
    "eslint airbnb react",
    "eslint airbnb typescript",
    "eslint airbnb extended"
  ],
  "homepage": "https://eslint-airbnb-extended.nishargshah.dev",
  "bugs": {
    "url": "https://github.com/eslint-config/airbnb-extended/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eslint-config/airbnb-extended.git",
    "directory": "packages/eslint-config-airbnb-extended"
  },
  "license": "MIT",
  "author": "Nisharg Shah <nishargshah3101@gmail.com>",
  "type": "module",
  "exports": {
    ".": "./dist/index.mjs",
    "./legacy": "./dist/legacy.mjs",
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.mts",
  "dependencies": {
    "@next/eslint-plugin-next": "^16.1.4",
    "@stylistic/eslint-plugin": "^5.7.0",
    "confusing-browser-globals": "^1.0.11",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.0.0",
    "typescript-eslint": "^8.53.1"
  },
  "devDependencies": {
    "@types/confusing-browser-globals": "^1.0.3",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "tsdown": "^0.19.0",
    "tsx": "^4.21.0",
    "@airbnb-extended/typescript-config": "3.0.0"
  },
  "peerDependencies": {
    "eslint": "^9.0.0"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": false
    }
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "scripts": {
    "prebuild": "pnpm check:updates",
    "build": "tsdown",
    "build:dev": "tsdown --watch",
    "check:updates": "tsx script/checkUpdates.ts",
    "format": "prettier . --check --log-level silent --ignore-path ../../.gitignore",
    "format:fix": "prettier . --write --log-level silent --ignore-path ../../.gitignore",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "script:lint": "bash -e ../../scripts/lint.sh --filter=eslint-config-airbnb-extended",
    "typecheck": "tsc --noEmit"
  }
}